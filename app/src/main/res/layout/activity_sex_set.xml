<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.exam.fs.push.widget.TitleView
            android:id="@+id/titleView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:left_ico_back="true"
            app:right_btn_isShow="true"
            app:right_btn_text="@string/app_complete"
            app:right_icon_res_ishow="false"
            app:right_text_ishow="false" />
        <com.exam.fs.push.widget.ShowInfoView
            android:id="@+id/btn_man"
            app:layout_constraintTop_toBottomOf="@+id/titleView"
            app:left_text="@string/app_man"
            app:right_icon="@drawable/sel_tab_sex"
            android:layout_width="match_parent"
            android:layout_height="@dimen/len_44"/>

        <com.exam.fs.push.widget.ShowInfoView
            android:id="@+id/btn_woman"
            app:layout_constraintTop_toBottomOf="@+id/btn_man"
            app:left_text="@string/app_woman"
            app:right_icon="@drawable/sel_tab_sex"
            android:layout_width="match_parent"
            android:layout_height="@dimen/len_44"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
